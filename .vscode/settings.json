    "**/*.egg": true,
    "**/*.manifest": true,
    "**/*.spec": true,
    "**/pip-log.txt": true,
    "**/pip-delete-this-directory.txt": true,
    "**/htmlcov/": true,
    "**/.tox/": true,
    "**/.coverage": true,
    "**/.coverage.*": true,
