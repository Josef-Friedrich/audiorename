    "**/.spyderproject": true,
    "**/.ropeproject": true,
    "_audiorename_backups/*": true,
    ".vscode/tags": true,
    ".tox/docs/**/*": true,
    ".tox/flake8/**/*": true,
    ".tox/py38/**/*": true,
    ".tox/py37/**/*": true,
    ".tox/py36/**/*": true
